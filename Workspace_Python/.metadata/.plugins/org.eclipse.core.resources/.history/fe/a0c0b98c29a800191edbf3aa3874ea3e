# -*- coding:UTF-8 -*-
# python 인코딩 방법. 맨위에 -*- coding:UTF-8 -*- 써주면 됨.

def func01():
    print('함수 1 입니다.')

def func02():
    return '함수 2 입니다.'

def func03():
    return {1:'a', 2:'b'}

if __name__ = '__main__':
    print('프로그램이 시작되면 가장 먼저 호출됨!!')
    # main (프로그램의 주 진입점)

    func01()
    print(func02)