subject = ['java', 'db', 'ui', 'web', 'spring', 'python']

for i in subject:
    print(i)
    
for i in range(1, 100):
    print(i)
    
# sep : 구분자
# end : 동작 후 할 행동
for i in range(1, 11):
    print(i,i, sep=', ', end='\t')
    
print('구구단')
for i in range(2,10):
    print('<', i, '단 >')
    for j in range(1,10):
        print(i,' * ', j, ' = ', i*j)
        
        
        
        